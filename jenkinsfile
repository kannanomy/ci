pipeline {
    agent any
    tools {
       maven 'maven'
    }
    stages {
        stage ('cleanWS') {
            steps {
              cleanWS()
           }
        }
        stage ('compile') {
            steps {
                sh 'mvn compile'
            }
        }
        stage ('test') {
            steps {
                sh 'test'
            }
        }
        stage ('unit test') {
            steps {
                sh 'mvn test'
            }
        }
    }  
}

     